	<div id = "first">
	<!-- 1. Mustache Expression -->
		<div>{{title}}</div>
		
		<!-- 2. directive -->
		<div v-text="title"></div>
	</div>
	
	
<script>
	var model = {title:'<h2>Encore Vue Test</h2>'}
	var app = new Vue({
		el : '#first',
		data : model
	});
</script>

